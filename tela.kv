#:import NoTransition kivy.uix.screenmanager.NoTransition



<Button>:
    border: (2, 2, 2, 2)
    border_width: 2
    border_color: 1, 0, 0, 1
    border_radius: [30]


<ScreenManager>:
    transition: NoTransition()
    MenuPrincipal:
        id: menu_1
        name: 'menu_1'

    NovosRegistros:
        id: registros
        name: 'registros'

    RegistrosRDMarcas:
        id: registrosrdmarcas
        name: 'registrosrdmarcas'

    RegistrosPerfumaria:
        id: registrosperfumaria
        name: 'registrosperfumaria'

    RegistrosDermo:
        id: registrosdermo
        name: 'registrosdermo'

    LimparDados:
        id: limpardados
        name: 'limpardados'

    LimparRD:
        id: limparrd
        name: 'limparrd'

    LimparPERFUMARIA:
        id: limparperfumaria
        name: 'limparperfumaria'

    LimparDERMO:
        id: limpardermo
        name: 'limpardermo'

    LimparTodasAsListas
        id: limpartodasaslistas
        name: 'limpartodasaslistas'

    ConsultaDeListas:
        id: consultarlistas
        name: 'consultarlistas'

    ConsultaRDMarcas:
        id: consultardmarcas
        name: 'consultardmarcas'

    ConsultaPerfumaria:
        id: consultaperfumaria
        name: 'consultaperfumaria'

    ConsultaDermo:
        id: consultadermo
        name: 'consultadermo'

    CriarBackup:
        id: criarbackup
        name: 'criarbackup'

    FecharPrograma:
        id: fecharPrograma
        name: 'fecharprograma'


<MenuPrincipal>:
    GridLayout:
        cols: 1
        spacing: 6
        padding: 10

        Button:
            text: 'NOVOS REGISTROS'
            font_size: 55
            color: 1, 1, 0, 1
            on_release:
                root.manager.current = 'registros'

        Button:
            text: 'EDITAR | LIMPAR'
            font_size: 55
            color: 1, 1, 0, 1
            on_release:
                root.manager.current = 'limpardados'

        Button:
            text: 'CONSULTAR LISTAS'
            font_size: 55
            color: 1, 1, 0, 1
            on_release:
                root.manager.current = 'consultarlistas'

        Button:
            text: 'CRIAR BACKUP'
            font_size: 55
            color: 1, 1, 0, 1
            on_release:
                root.manager.current = 'criarbackup'

        Button:
            text: 'SAIR'
            font_size: 55
            color: 1, 0, 0, 1
            size_hint: (0.0, 0.9)
            on_release:
                app.stop()


# Opções do Menu principal (REGISTROS)
<NovosRegistros>:
    GridLayout:
        cols: 1
        spacing: 6
        padding: 10

        Label:
            text: 'REGISTROS'
            font_size: 60
            orientation: 'horizontal'
            size_hint: (0.5, 0.5)

        Button:
            text: 'RD MARCAS'
            font_size: 55
            color: 1, 1, 0, 1
            on_release:
                root.manager.current = 'registrosrdmarcas'

        Button:
            text: 'PERFUMARIA'
            font_size: 55
            color: 1, 1, 0, 1
            on_release:
                root.manager.current = 'registrosperfumaria'

        Button:
            text: 'DERMO'
            font_size: 55
            color: 1, 1, 0, 1
            size_hint: (0.0, 0.9)
            on_release:
                root.manager.current = 'registrosdermo'

        Button:
            text: 'VOLTAR'
            font_size: 55
            on_release:
                root.manager.current = 'menu_1'


<RegistrosRDMarcas>:
    BoxLayout:
        orientation: 'vertical'
        spacing: 10
        padding: 10

        Label:
            text: 'Preencha os campos (RD-MARCAS)'
            font_size: 35
            color: 1, 1, 0, 1
            size_hint: (1, None)
            text_size: (None, None)

        BoxLayout:
            orientation: 'horizontal'
            size_hint: (1, None)
            height: '30dp'

            Label:
                size_hint: (0.3, 1)
                text: 'Data'

            TextInput:
                id: data_input
                size_hint: (0.7, 1)
                multiline: False
                hint_text: "00/00/0000"

        BoxLayout:
            orientation: 'horizontal'
            size_hint: (1, None)
            height: '30dp'

            Label:
                size_hint: (0.3, 1)
                text: 'Meta* (Dia)'

            TextInput:
                id: meta_input
                size_hint: (0.7, 1)
                input_filter: 'float'
                multiline: False
                hint_text: "R$ 1000.00"

        BoxLayout:
            orientation: 'horizontal'
            size_hint: (1, None)
            height: '30dp'

            Label:
                size_hint: (0.3, 1)
                text: 'Vendas* (Dia)'

            TextInput:
                id: venda_input
                size_hint: (0.7, 1)
                input_filter: 'float'
                multiline: False
                hint_text: "R$ 1000.00"

        BoxLayout:
            orientation: 'horizontal'

            Button:
                text: 'VOLTAR'
                font_size: 55
                size_hint: (0.5, 0.2)
                on_release:
                    root.manager.current = 'registros'

            Button:
                text: 'FINALIZAR'
                font_size: 55
                size_hint: (0.7, 0.2)
                on_release:
                    root.pega_input_rdmarcas()


